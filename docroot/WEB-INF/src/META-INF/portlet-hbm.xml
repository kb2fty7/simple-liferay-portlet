<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.kb2fty7.testapplication.model.UserNote" />
	<class name="com.kb2fty7.testapplication.model.impl.UserNoteImpl" table="test_portlet_UserNote">
		<cache usage="read-write" />
		<id name="id" column="id_" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="lastname" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="question" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>